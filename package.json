{
    "name": "fhevm-starterkit",
    "version": "1.0.0",
    "private": true,
    "description": "A curated collection of FHEVM starter templates and project generators for building privacy-preserving smart contracts.",
    "author": "M.E.W",
    "license": "MIT",
    "scripts": {
        "template:init": "ts-node scripts/cli.ts template:init",
        "template:update": "ts-node scripts/cli.ts template:update",
        "template:reset": "ts-node scripts/cli.ts template:reset",
        "template:clean": "ts-node scripts/cli.ts template:clean",
        "template:build-ui": "ts-node scripts/cli.ts template:build-ui",
        "starter:list": "ts-node scripts/cli.ts starter:list",
        "start": "npm run template:init && npm run template:build-ui",
        "starter:create": "ts-node scripts/cli.ts starter:create",
        "docs:generate": "ts-node scripts/docs-generate.ts",
        "web": "npm --prefix apps/web run dev",
        "check": "ts-node scripts/check.ts",
        "build:web": "npm --prefix apps/web run build",
        "lint": "eslint .",
        "format": "prettier --write .",
        "debug:logger": "ts-node scripts/debug/logger-test.ts",
        "generate:schema": "tsx tools/generateJsonSchema.ts"
    },
    "devDependencies": {
        "@types/node": "^20.19.26",
        "ajv": "^8.12.0",
        "rimraf": "^6.1.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.8.3",
        "zod-to-json-schema": "^3.25.0"
    },
    "dependencies": {
        "commander": "^14.0.2",
        "enquirer": "^2.4.1",
        "zod": "^4.2.1"
    }
}
