{
    "name": "fhevm-starterkit",
    "version": "1.0.0",
    "private": true,
    "description": "A curated collection of FHEVM starter templates and project generators for building privacy-preserving smart contracts.",
    "author": "Your Name",
    "license": "MIT",
    "scripts": {
        "template:init": "ts-node scripts/template-init.ts",
        "template:update": "ts-node scripts/template-update.ts",
        "template:reset": "ts-node scripts/template-reset.ts",

        "setup:hardhat": "ts-node scripts/setup-hardhat.ts",
        "setup:frontend": "ts-node scripts/setup-frontend.ts",
        
        "docs:generate": "ts-node scripts/docs-generate.ts",
        "starter:use": "ts-node scripts/starter-use.ts",
        "starter:init": "ts-node scripts/starter-init.ts",
        "web": "npm --prefix apps/web run dev",
        "check": "ts-node scripts/check.ts",
        "build:web": "npm --prefix apps/web run build",
        "lint": "eslint .",
        "format": "prettier --write .",
        "debug:logger": "ts-node scripts/debug/logger-test.ts"
    },
    "devDependencies": {
        "@types/node": "^20.19.26",
        "ajv": "^8.12.0",
        "ts-node": "^10.9.2",
